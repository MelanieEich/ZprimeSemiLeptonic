            <OutputTree Name="AnalysisTree" />  
        </InputData>

        <InputData Lumi="1" NEventsMax="-1" Type="MC" Version="ZP3000w30" Cacheable="False">
            <In FileName="&PRESELdir;/uhh2.AnalysisModuleRunner.MC.ZP3000w30.root" Lumi="0.0"/> 
